// https://takeuforward.org/plus/data-structures-and-algorithm/beginner-problems/patterns/pattern-14

class Solution {
public:

//TC - O(n2)
    void pattern14(int n) {
        for(int i = 1; i <= n; i++){
            char ch = 'A';
            for(int j = 1; j <= i; j++){
                cout << ch;
                ch++;
            }
            cout << endl;
        }
    }
};