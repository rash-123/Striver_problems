// https://takeuforward.org/plus/data-structures-and-algorithm/beginner-problems/language-basics/if-else-adult-teen-problem

class Solution {
public:

//TC - O(1)
    void isAdult(int age) {
        if(age >= 18){
            cout << "Adult" << endl;
        }
        else{
            cout << "Teen" << endl;
        }
    }
};