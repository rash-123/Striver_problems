// https://takeuforward.org/plus/data-structures-and-algorithm/beginner-problems/patterns/pattern-2

class Solution {
public:

//TC - O(n2)
    void pattern2(int n) {
        for(int i = 1; i <= n; i++){
            for(int j = 1; j <= i; j++){
                cout << "*";
            }
            cout << endl;
        }
    }
};