// https://takeuforward.org/plus/data-structures-and-algorithm/beginner-problems/patterns/pattern-13

class Solution {
public:

//TC - O(n2)
    void pattern13(int n) {
        int k = 1;
        for(int i = 1; i <= n; i++){
            for(int j = 1; j <= i; j++){
                cout << k << " ";
                k++;
            }
            cout << endl;
        }
    }
};