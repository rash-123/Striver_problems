// https://takeuforward.org/plus/data-structures-and-algorithm/beginner-problems/patterns/pattern-16

class Solution {
public:

//TC - O(n2)
    void pattern16(int n) {
        char ch = 'A';
        for(int i = 1; i <= n; i++){
            for(int j = 1; j <= i; j++){
                cout << ch;
            }
            ch++;
            cout << endl;
        }
    }
};