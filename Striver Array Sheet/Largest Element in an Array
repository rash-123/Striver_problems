https://www.geeksforgeeks.org/problems/largest-element-in-array4009/0

class Solution
{
public:

//Brute force - Sort the array and the return the last element of the array
// TC - O(NlogN) SC - O(1)

//Better 
//TC - O(N) SC - O(1)
    int largest(vector<int> &arr, int n)
    {
        int largest = arr[0];
            for(int i=0;i<n;i++){
                if(arr[i]>largest){
                    largest = arr[i];
                }
            }
        return largest;
    }
};