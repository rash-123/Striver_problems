// https://takeuforward.org/plus/data-structures-and-algorithm/beginner-problems/patterns/pattern-5

class Solution {
public:

//TC - O(n2)
    void pattern5(int n) {
        for(int i = 1; i <= n; i++){
            for(int j = n; j >= i; j--){
                cout << "*";
            }
            cout << endl;
        }
    }
};